            ],
            "dlvLoadConfig": {
                "followPointers": true,
                "maxVariableRecurse": 1,
                "maxStringLen": 512,
                "maxArrayValues": 64,
                "maxStructFields": -1
            },
            "dlvFlags": [
                "--check-go-version=false"
            ]
        }
    ]

    "version": "0.2.0",
    "configurations": [
        {
            "name": "st",
            "type": "go",
            "request": "launch",
            // "debugAdapter": "dlv-dap",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env": {},
            "args": [
                "st"
            ],
            "dlvLoadConfig": {
                "followPointers": true,
                "maxVariableRecurse": 1,
                "maxStringLen": 512,
                "maxArrayValues": 64,
                "maxStructFields": -1
            },
            "dlvFlags": [
                "--check-go-version=false"
            ]
        },
        {
            "name": "min",
